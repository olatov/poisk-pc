      </Unit>
      <Unit>
        <Filename Value="debugger.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Debugger"/>
      </Unit>
      <Unit>
        <Filename Value="keyboard.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="Keyboard"/>
      </Unit>
      <Unit>
        <Filename Value="appsettings.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="AppSettings"/>
      </Unit>
      <Unit>
        <Filename Value="xtidecontroller.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="XTIDEController"/>
      </Unit>
      <Unit>
        <Filename Value="poisk.rc"/>
        <IsPartOfProject Value="True"/>
      </Unit>
    </Units>
  </ProjectOptions>
  <CompilerOptions>
    <Version Value="11"/>
    <Target>
      <Filename Value="bin/poisk"/>
    </Target>
    <SearchPaths>
      <IncludeFiles Value="$(ProjOutDir)"/>
      <UnitOutputDirectory Value="lib/$(TargetCPU)-$(TargetOS)"/>
